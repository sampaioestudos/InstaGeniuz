{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "index.tsx",
      "use": "@vercel/static"
    },
    {
      "src": "App.tsx",
      "use": "@vercel/static"
    },
    {
      "src": "types.ts",
      "use": "@vercel/static"
    },
    {
      "src": "constants.ts",
      "use": "@vercel/static"
    },
    {
      "src": "metadata.json",
      "use": "@vercel/static"
    },
    {
      "src": "components/**",
      "use": "@vercel/static"
    },
    {
      "src": "hooks/**",
      "use": "@vercel/static"
    },
    {
      "src": "services/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
